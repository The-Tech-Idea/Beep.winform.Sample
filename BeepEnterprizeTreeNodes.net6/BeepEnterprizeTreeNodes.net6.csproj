<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <BaseOutputPath>bin\</BaseOutputPath>
    <DebugType>full</DebugType>
    <RootNamespace>BeepEnterprizeTreeNodes</RootNamespace>
  </PropertyGroup>
	<Target Name="CopyPackage" AfterTargets="Pack">
		<Copy SourceFiles="$(OutputPath)..\$(PackageId).$(PackageVersion).nupkg" DestinationFolder="C:\Users\f_ald\source\repos\Nuget.Local" />
	</Target>

	<ItemGroup>
    <Compile Include="..\BeepEnterprizeTreeNodes\**\*">
      <Link>%(RecursiveDir)/%(FileName).cs</Link>
    </Compile>
</ItemGroup>
	<ItemGroup>
	  <Compile Remove="..\BeepEnterprizeTreeNodes\bin\Debug\**" />
	  <Compile Remove="..\BeepEnterprizeTreeNodes\obj\**" />
	  <Compile Remove="..\BeepEnterprizeTreeNodes\Properties\**" />
	  <Compile Remove="bin\**" />
	  <EmbeddedResource Remove="bin\**" />
	  <None Remove="bin\**" />
	</ItemGroup>
	<ItemGroup>
	  <Compile Remove="..\BeepEnterprizeTreeNodes\app.config" />
	  <Compile Remove="..\BeepEnterprizeTreeNodes\BeepEnterprizeTreeNodes.csproj" />
	</ItemGroup>
	<ItemGroup>
	  <PackageReference Include="TheTechIdea.Beep.DataManagementEngineNet6" Version="1.0.0.12" />
	  <PackageReference Include="TheTechIdea.Beep.DataManagementModelsNet6" Version="1.0.0.5" />
	  <PackageReference Include="TheTechIdea.Beep.Vis.ModulesNet6" Version="1.0.0.5" />
	</ItemGroup>
	<ItemGroup>
	  <Folder Include="App\" />
	</ItemGroup>
</Project>
